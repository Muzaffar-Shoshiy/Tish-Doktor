            <div id="layoutSidenav_content">
                <main>
                    <div class="container-fluid px-4">
                        <h1 class="mt-4"><i class="fa-solid fa-circle-exclamation text-danger me-4"></i>Rejected Doctors</h1>
                        <ol class="breadcrumb mb-4">
                            <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
                            <li class="breadcrumb-item active">Tables</li>
                        </ol>
                        <div class="card mb-4">
                            <div class="card-header">
                                <i class="fas fa-table me-1"></i>
                                Rejected Doctors
                            </div>
                            <div class="card-body">
                                <table id="datatablesSimple">
                                    <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>CHAT_ID</th>
                                            <th>ACTION</th>
                                            <th>USERNAME</th>
                                            <th>FIRST NAME</th>
                                            <th>FULL NAME</th>
                                            <th>CITY</th>
                                            <th>DISTRICT</th>
                                            <th>EXPERIENCE</th>
                                            <th>FEE</th>
                                            <th>PHONE</th>
                                            <th>LATITUDE</th>
                                            <th>LONGITUDE</th>
                                            <th>PHOTO</th>
                                            <th>DIPLOMA</th>
                                            <th>ACCEPTED</th>
                                            <th>CREATED AT</th>
                                        </tr>
                                    </thead>
                                    <!-- <tfoot>
                                        <tr>
                                            <th>Name</th>
                                            <th>Position</th>
                                            <th>Office</th>
                                            <th>Age</th>
                                            <th>Start date</th>
                                            <th>Salary</th>
                                        </tr>
                                    </tfoot> -->
                                    <tbody>
                                        <% data.forEach(entry => { %>
                                            <% if(entry.diploma && entry.photo && entry.longitude && entry.latitude && entry.phone && entry.fee && entry.experience && entry.district && entry.city && entry.full_name){ %>
                                            <tr>
                                                <td><%=  entry.id  %></td>
                                                <td><%=  entry.chat_id  %></td>
                                                <td class="text-center">
                                                    <form action="/unaccepteddoctors" method="post">
                                                        <input type="hidden" name="chat_id" id="chat_id" value="<%=entry.chat_id%>">
                                                        <button type="submit" class="btn btn-success">Accept</button>
                                                    </form><br>
                                                    <!-- <form action="#">
                                                        <input type="hidden" name="id" id="id">
                                                        <button type="submit" class="btn btn-warning">Warning</button>
                                                    </form> -->
                                                </td>
                                                <td><%=  entry.username  %></td>
                                                <td><%=  entry.first_name  %></td>
                                                <td><%=  entry.full_name  %></td>
                                                <td><%=  entry.city  %></td>
                                                <td><%=  entry.district  %></td>
                                                <td><%=  entry.experience  %></td>
                                                <td><%=  entry.fee  %></td>
                                                <td><%=  entry.phone  %></td>
                                                <td><%=  entry.latitude  %></td>
                                                <td><%=  entry.longitude  %></td>
                                                <td><%=  entry.photo  %></td>
                                                <td><%=  entry.diploma  %></td>
                                                <td><%=  entry.accepted  %></td>
                                                <td><%=  entry.created_at  %></td>
                                            </tr>
                                            <% } %>
                                        <% }) %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
